                    span(aria-hidden="true") ×

                h2 Регистрация

                .form-group
                    label Фамилия
                    input#name-field.form-control(type='text', name='lastname', placeholder='Введите Вашу фамилию...')
                .form-group
                    label Имя
                    input.form-control(type='text', name='firstname', placeholder='Введите Ваше имя...')
                .form-group
                    label Отчество
                    input.form-control(type='text', name='middlename', placeholder='Введите Ваше отчество...')
                .form-group
                    label Телефон
                    input.form-control(type='tel', name='phone', placeholder='(XXX) XXX - XX - XX')
                .form-group
                    label E-mail
                    input.form-control(type='email', name='email', placeholder='Введите Ваш e-mail...')
                .form-group
                    button.btn.btn-primary.btn-md.btn-cta.btn-signup(type="submit") Отправить
